  <item_list>
    <item>
      <name><![CDATA[fieldset1]]></name>
      <default><![CDATA[<fieldset>
  <legend></legend>
  <div class="fieldvars">
    <input type="hidden" name="fieldset" role="field" value="1" />
    <div class="tinyForm">
      <ul>
        <li class="flex">
          <span class="column"><input is="jtbc-input" type="text" name="extra_min" role="field" placeholder="min" width="100%" /></span>
          <span class="column"><input is="jtbc-input" type="text" name="extra_max" role="field" placeholder="max" width="100%" /></span>
          <span class="column"><input is="jtbc-input" type="text" name="extra_step" role="field" placeholder="step" width="100%" /></span>
        </li>
      </ul>
    </div>
  </div>
</fieldset>]]></default>
    </item>
