    <div class="tinyForm">
      <ul>
        <li><jtbc-field-code-editor name="comment_columns" role="field" placeholder="columns" height="180" mode="javascript" mime="application/json" fixed="fixed"></jtbc-field-code-editor></li>
      </ul>
